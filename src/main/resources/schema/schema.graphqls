type Employee {
	id:Int!
	first_name: String!
	last_name: String!
	gender: Gender
	birth_date: Date
	hire_date: Date	
    department: Department!
}

type Department {
  id : Int!
  dept_name : String!
  employees:[Employee!]
}

enum Gender {
 M
 F 
}

scalar Date

type Query {
   
   employee(id : Int) : Employee
   employees :[Employee!]
   departments :[Department!]
   getEmployeesByDeptId(deptId : Int) :[Employee!]
}